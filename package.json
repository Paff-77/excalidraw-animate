{
  "name": "excalidraw-animate",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@excalidraw/eslint-config": "1.0.0",
    "@excalidraw/prettier-config": "1.0.2",
    "@sentry/browser": "6.2.5",
    "@sentry/integrations": "6.2.5",
    "browser-fs-access": "0.16.4",
    "clsx": "1.1.1",
    "firebase": "8.3.3",
    "i18next-browser-languagedetector": "6.0.1",
    "lodash.throttle": "4.1.1",
    "nanoid": "3.1.22",
    "open-color": "1.8.0",
    "pako": "1.0.11",
    "png-chunk-text": "1.0.0",
    "png-chunks-encode": "1.0.0",
    "png-chunks-extract": "1.0.0",
    "points-on-curve": "0.2.0",
    "pwacompat": "2.0.17",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "roughjs": "4.3.1",
    "sass": "1.32.10",
    "socket.io-client": "2.3.1",
    "typescript": "4.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@types/dom-mediacapture-record": "1.0.7",
    "@types/jest": "26.0.19",
    "@types/lodash.throttle": "4.1.6",
    "@types/nanoid": "2.1.0",
    "@types/pako": "1.0.1",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/socket.io-client": "1.4.34",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "3.3.0",
    "jest-canvas-mock": "2.3.0",
    "pepjs": "0.5.3",
    "prettier": "2.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "yarn run test:all",
    "test:all": "yarn run test:type && yarn run test:lint && yarn run test:format && yarn run test:app --watchAll=false",
    "test:type": "tsc",
    "test:lint": "eslint --max-warnings=0 --ext .js,.ts,.tsx .",
    "test:format": "prettier 'src/*.(js|ts|tsx|css|scss)' --list-different",
    "test:app": "react-scripts test --env=jsdom --passWithNoTests --testPathIgnorePatterns=src/excalidraw",
    "preinstall": "git submodule update --init --recursive"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(roughjs|points-on-curve|path-data-parser|points-on-path|browser-fs-access)/)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
